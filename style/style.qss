/*tab页设置*/
QTabWidget::pane{       /*主界面*/
        border:none;
        background:white;
}
QTabWidget::tab-bar{        /*tab所在位置*/
        alignment:right;
      /* left:500px;*/
}
QTabBar::tab{
        border: 0px solid #C4C4C3;     /*边界宽度和颜色*/
        border-top-color:#212e31;       /*上边界颜色*/
        background:#212e31;
        color:white;
        font-family:"Nuosu SIL";
        font-size:11px;
        font-weight:bold;
        min-height:32 px;
        min-width:73 px;
}
QTabBar::tab:hover{                             /*tab鼠标悬停的颜色*/
        background:rgb(255, 255, 255, 100);
}
QTabBar::tab:selected{                      /*tab选中的颜色*/
        border-color: white;
        background:white;
        color:black;
        font-family:"PT Sans Narrow";
        font-size:12px;
        font-weight:normal;
        min-height:32 px;
        min-width:73 px;
}
/*安全策略的tab设置*/
QTabWidget#tabWidget::pane{       /*主界面*/
        border:1px solid #C4C4C3;
        background:white;
}
QTabWidget#tabWidget::tab-bar{        /*tab所在位置*/
        alignment:right;
      /* left:500px;*/
}
QTabWidget#tabWidget>QTabBar::tab{
        border: 0px solid #50a495;     /*边界宽度和颜色*/
        border-top-color:#50a495;       /*上边界颜色*/
         border-bottom-color:white;       /*下边界颜色*/
        background:#eeeeee;
        color:black;
        font-family:"Cantarell";
        font-size:11px;
        font-weight:bold;
        min-height:15 px;
        min-width:73 px;
}

QTabWidget#tabWidget>QTabBar::tab:hover{                             /*tab鼠标悬停的颜色*/
        background:rgb(80, 164, 149, 100);   /*#eeeeee*/
        font-weight:normal;
        color:black;
}
QTabWidget#tabWidget>QTabBar::tab:selected{                      /*tab选中的颜色*/
        border-color: gray;
        background:#50a495;
        color:black;
        font-family:"PT Sans Narrow";
        font-size:12px;
        font-weight:normal;
        min-height:15 px;
        min-width:73 px;
}


