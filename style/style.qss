/*按钮QPushButton*/
QPushButton#freshButton {
    border: 1px solid white;
    border-radius: 0px;
    color:white;
    font-weight:bold;
    background-color:#50a495;
    min-width:70px;
}
QPushButton#freshButton:hover {
    border: 1px solid white;
    border-radius: 0px;
    color:white;
    font-weight:normal;
    background-color:#2a564e;
}
QPushButton#freshButton:pressed {
    border: 1px solid white;
    border-radius: 0px;
    color:white;
    font-weight:bold;
    background-color:#50a495;
}

